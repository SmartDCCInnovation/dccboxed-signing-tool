        <xs:sequence>
            <!-- Returned by device alert 0x8F87  -->
            <xs:element name="AuxiliaryControllerN" type="ra:range_1_5"/>
            <xs:element name="ResultingLevel" type="ra:AuxiliaryControllerLevel"/>
            <xs:element minOccurs="0" name="InputFromControlledLoad" type="ra:NoType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="DeviceAlertMessagePayload">
        <xs:choice>
            <xs:element name="FirmwareVerificationDeviceAlert"
                type="ra:FirmwareVerificationDeviceAlertType"/>
            <xs:element name="BillingDataLogDeviceAlert" type="ra:BillingDataLogDeviceAlertType"/>
            <xs:element name="FutureDatedCommandOutcomeDeviceAlert"
                type="ra:FutureDatedCommandOutcomeDeviceAlertType"/>
            <xs:element name="SupplyOutageRestoreAlert" type="ra:SupplyOutageRestoreAlertType"/>
            <xs:element name="SmartMeterIntegrityIssueWarningDeviceAlert"
                type="ra:SmartMeterIntegrityIssueWarningDeviceAlertType"/>
            <xs:element name="CommandNotSupportedbyDevice"
                type="ra:CommandNotSupportedbyDevice"/>
            <xs:element name="OperationalUpdateDeviceAlert" type="ra:OperationalUpdateDeviceAlert"/>
            <xs:element name="LimitAPCLevelCommandProcessedDeviceAlert"
                type="ra:LimitAPCLevelCommandProcessedDeviceAlert"/>
            <xs:element name="LimitAPCLevelEndedDeviceAlert" type="ra:LimitAPCLevelEndedDeviceAlert"
            />
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="DeviceAlertContentType">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="GBCSHexAlertCode" type="xs:hexBinary"/>
            <xs:element maxOccurs="1" minOccurs="1" name="AlertDescription">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="250"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <!-- Note that there is only an identified GBCS use case for the small number of alerts 
            	 which return additional data. -->
            <xs:element name="Timestamp" type="xs:dateTime"/>
            <xs:element minOccurs="0" name="Payload" type="ra:DeviceAlertMessagePayload"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="DeviceAlertMessageType">
        <xs:sequence>
            <!-- GBCSData is for use where there is sensitive data in an alert payload and a second pass to Parse software will be required -->
            <xs:element ref="ra:GBCSData" maxOccurs="100" minOccurs="0"/>
            <!-- DeviceAlertContent will carry the data of the alert and in most cases will be the only element present at this level -->
            <xs:element maxOccurs="1" minOccurs="0" name="DeviceAlertContent"
                type="ra:DeviceAlertContentType"/>
        </xs:sequence>
    </xs:complexType>

    <!-- Critical Sensitive Alert -->
    <xs:complexType name="BillingDataLogDeviceAlertType">
        <xs:choice>
            <xs:element name="ESMEBillingDataLogEntry" maxOccurs="1"
                type="ra:ESMEBillingDataLogType"> </xs:element>
            <xs:element name="GSMEBillingDataLogEntry" maxOccurs="1"
                type="ra:GSMEBillingDataLogType"> </xs:element>
        </xs:choice>
    </xs:complexType>

    <!-- Read Billing Data Log -->
    <xs:complexType name="RetrieveBillingCalendarTriggeredBillingDataLogRsp">
        <!-- SR 4.4.3 Retrieve Billing Calendar Triggered Billing Data Log -->
        <xs:complexContent>
            <xs:extension base="ra:ServiceRequestBaseType">
                <xs:choice>
                    <xs:element name="Electricity">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="ESMEBillingDataLogEntry" maxOccurs="12"
                                    minOccurs="0" type="ra:ESMEBillingDataLogType"> </xs:element>
                                <xs:element name="ESMEBillingPrepaymentDataLogEntry" maxOccurs="12"
                                    minOccurs="0" type="ra:PrepaymentOperationalDataType">
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Gas">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="GSMEBillingDataLogEntry" maxOccurs="12"
                                    minOccurs="0" type="ra:GSMEBillingDataLogType"> </xs:element>
                                <xs:element name="GSMEBillingPrepaymentDataLogEntry" maxOccurs="12"
                                    minOccurs="0" type="ra:PrepaymentOperationalDataType">
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:choice>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ESMEBillingDataLogType">
        <xs:sequence>
            <xs:element maxOccurs="1" name="Timestamp" type="xs:dateTime" minOccurs="0"/>
            <xs:element maxOccurs="1" name="ActiveImportRegisterValueConsumption" type="xs:integer"
                minOccurs="0"/>
            <xs:element maxOccurs="1" name="SecondaryActiveImportRegisterValueConsumption"
                type="xs:integer" minOccurs="0"/>
            <xs:element maxOccurs="48" name="TariffTOURegisterMatrixValue"
                type="ra:IntegerWithIndex" minOccurs="0"/>

            <xs:element maxOccurs="4" name="SecondaryTariffTOURegisterMatrixValue"
                type="ra:IntegerWithIndex" minOccurs="0"/>

            <xs:element maxOccurs="8" name="TariffTOUBlock1RegisterMatrixValue"
                type="ra:IntegerWithIndex" minOccurs="0"/>

            <xs:element maxOccurs="8" name="TariffTOUBlock2RegisterMatrixValue"
                type="ra:IntegerWithIndex" minOccurs="0"/>

            <xs:element maxOccurs="8" name="TariffTOUBlock3RegisterMatrixValue"
                type="ra:IntegerWithIndex" minOccurs="0"/>

            <xs:element maxOccurs="8" name="TariffTOUBlock4RegisterMatrixValue"
                type="ra:IntegerWithIndex" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="GSMEBillingDataLogType">
        <xs:sequence>
            <xs:element maxOccurs="1" name="Timestamp" type="xs:dateTime" minOccurs="0"/>
            <xs:element maxOccurs="1" name="ActiveImportRegisterValueConsumption" type="xs:decimal"
                minOccurs="0"/>
            <xs:element maxOccurs="4" name="BlockRegisterMatrixValue" type="ra:DecimalWithIndex"
                minOccurs="0"/>
            <xs:element maxOccurs="4" name="TariffTOURegisterMatrixValue" type="ra:DecimalWithIndex"
                minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="GBCSResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Header" type="ra:HeaderType"/>
                <xs:element name="Body">
                    <xs:complexType>
                        <xs:choice>
                            <xs:element name="ResponseMessage" type="ra:ResponsePayload"/>
                            <xs:element name="DeviceAlertMessage" type="ra:DeviceAlertMessageType"/>
                        </xs:choice>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="schemaVersion" type="xs:decimal" use="required"/>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="ExecutionOutcome">
        <xs:sequence>
            <xs:element maxOccurs="1" name="AuthorisingRemotePartyOriginatorCounter"
                type="xs:nonNegativeInteger" minOccurs="0"/>
            <xs:element maxOccurs="1" name="CredentialsReplacementMode" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- SR 8.5 -->
                        <xs:enumeration value="ACBByACB"/>
                        <!-- SR 6.15.1, 6.21 -->
                        <xs:enumeration value="SupplierBySupplier"/>
                        <xs:enumeration value="NetworkOperatorByNetworkOperator"/>
                        <!-- SR 6.15.1 -->
                        <xs:enumeration value="LoadControllerBySupplier"/>
                        <!-- SR 6.23 -->
                        <xs:enumeration value="SupplierByTransCoS"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="RemotePartySeqNumberChange" minOccurs="0"
                type="ra:RemotePartySeqNumberChange"/>
            <xs:element maxOccurs="3" minOccurs="0" name="ReplacementOutcome"
                type="ra:ReplacementOutcome"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="RemotePartySeqNumberChange">
        <xs:sequence>
            <xs:element name="RemotePartyRole" type="ra:RemotePartyRole" minOccurs="0"/>
            <xs:element name="RemotePartyFloorSeqNumber" type="xs:nonNegativeInteger" minOccurs="0"/>
            <xs:element minOccurs="0" name="RemotePartyPrepaymentTopUpFloorSeqNumber"
                type="xs:nonNegativeInteger"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ReplacementOutcome">
        <xs:sequence>
            <xs:element name="StatusCode" type="ra:StatusASN1" minOccurs="0"/>
            <xs:element name="CertificateType" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- Applicability depends on SR and Device Type -->
                        <xs:enumeration value="DigitalSigning"/>
                        <xs:enumeration value="KeyAgreement"/>
                        <xs:enumeration value="KeyAgreementTopUp"/>
                        <xs:enumeration value="KeyCertSign"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="RemotePartyRole" type="ra:RemotePartyRole" minOccurs="0"/>
            <xs:element name="ExistingRemotePartyID" type="ra:EUI" minOccurs="0"/>
            <xs:element name="NewRemotePartyID" minOccurs="0" type="ra:EUI"/>
            <xs:element name="ExistingCertificateHash" type="ra:SHA1" minOccurs="0"/>
            <xs:element name="NewCertificateHash" type="ra:SHA1" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="SHA1">
        <xs:restriction base="xs:base64Binary"/>
    </xs:simpleType>

    <xs:simpleType name="AuxiliaryControllerLevel">
        <xs:restriction base="xs:unsignedShort">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="100"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="SetAuxiliaryControllerStateRsp">
        <xs:complexContent>
            <xs:extension base="ra:ServiceRequestBaseType">
                <!-- SR 7.13 Set Auxiliary Controller State --> </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ReadAuxiliaryControllerConfigurationDataRsp">
        <!-- SR 7.14 Read Auxiliary Controller Configuration Data -->
        <xs:complexContent>
            <xs:extension base="ra:ServiceRequestBaseType">
                <xs:sequence>
                    <xs:element name="AuxiliaryControllerDescription" maxOccurs="5" minOccurs="0"
                        type="ra:AuxiliaryControllerDescription"/>
                    <xs:element maxOccurs="1" minOccurs="0"
                        name="AuxiliaryControllerSpecialDayDefinitions"
                        type="ra:AuxiliaryControllerSpecialDayDefinitions"/>
                    <xs:element minOccurs="0" name="AuxiliaryControllerSchedule"
                        type="ra:AuxiliaryControllerSchedule" maxOccurs="120"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AuxiliaryControllerDescription">
        <xs:sequence maxOccurs="1" minOccurs="1">
            <xs:element name="AuxiliaryControlDescription">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="22"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="auxiliaryControllerN" type="ra:range_1_5" use="required"/>
    </xs:complexType>

    <xs:complexType name="ReadAuxiliaryControllerOperationalDataRsp">
        <!-- SR 7.15 Read Auxiliary Controller Operational Data -->
        <xs:complexContent>
            <xs:extension base="ra:ServiceRequestBaseType">
                <xs:sequence>
                    <xs:element name="AuxiliaryControllerOperationalData" maxOccurs="5" minOccurs="0"
                        type="ra:AuxiliaryControllerOperationalData"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="AuxiliaryControllerOperationalData">
        <xs:sequence>
            <xs:element minOccurs="0" name="TypeAndIdentifier"
                type="ra:TypeAndIdentifier"/>
            <xs:element minOccurs="0" name="AuxiliaryControllerStateAndAssociatedInformation"
                type="ra:AuxiliaryControllerStateAndAssociatedInformation"/>
        </xs:sequence>
        <xs:attribute name="auxiliaryControllerN" type="ra:range_1_5"/>
    </xs:complexType>
    <xs:complexType name="TypeAndIdentifier">
        <xs:sequence>
            <xs:element name="AuxiliaryControllerType">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="APC"/>
                        <xs:enumeration value="ALCS"/>
                        <xs:enumeration value="HCALCS"/>
                        <xs:enumeration value="None"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="HCALCSID" type="ra:EUI" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="AuxiliaryControllerStateAndAssociatedInformation">
        <xs:sequence>
            <xs:element name="OutputState" type="ra:AuxiliaryControllerLevel" minOccurs="0"/>
            <xs:element name="InputState" type="ra:AuxiliaryControllerLevel" minOccurs="0"/>
            <xs:element name="StateAndAssociatedInformation" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="72000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="LimitAPCLevelRsp">
        <xs:complexContent>
            <xs:extension base="ra:ServiceRequestBaseType">
                <!-- SR 7.16 Limit APC Level --> </xs:extension>
        </xs:complexContent>
    </xs:complexType>
</xs:schema>
