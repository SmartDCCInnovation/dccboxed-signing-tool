    <xs:simpleType name="ReactiveEnergyUnit">
        <xs:restriction base="xs:string">
            <!-- 32 is only valid value in GBCS payload, which means varh -->
            <xs:enumeration value="varh"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="GasConsumptionUnit">
        <xs:restriction base="xs:string">
            <xs:enumeration value="m3"/>
        </xs:restriction>
    </xs:simpleType>


