    <xs:complexType name="StatusCOSEM">
        <xs:choice>
            <xs:element name="DataAccessResult">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="success"/>
                        <xs:enumeration value="hardware-fault"/>
                        <xs:enumeration value="temporary-failure"/>
                        <xs:enumeration value="read-write-denied"/>
                        <xs:enumeration value="object-undefined"/>
                        <xs:enumeration value="object-class-inconsistent"/>
                        <xs:enumeration value="object-unavailable"/>
                        <xs:enumeration value="type-unmatched"/>
                        <xs:enumeration value="scope-of-access-violated"/>
                        <xs:enumeration value="data-block-unavailable"/>
                        <xs:enumeration value="long-get-aborted"/>
                        <xs:enumeration value="no-long-get-in-progress"/>
                        <xs:enumeration value="long-set-aborted"/>
                        <xs:enumeration value="no-long-set-in-progress"/>
                        <xs:enumeration value="data-block-number-invalid"/>
                        <xs:enumeration value="other-reason"/>
                        <xs:enumeration value="response-code-not-known"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="ActionResult">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="success"/>
                        <xs:enumeration value="hardware-fault"/>
                        <xs:enumeration value="temporary-failure"/>
                        <xs:enumeration value="read-write-denied"/>
                        <xs:enumeration value="object-undefined"/>
                        <xs:enumeration value="object-class-inconsistent"/>
                        <xs:enumeration value="object-unavailable"/>
                        <xs:enumeration value="type-unmatched"/>
                        <xs:enumeration value="scope-of-access-violated"/>
                        <xs:enumeration value="data-block-unavailable"/>
                        <xs:enumeration value="long-action-aborted"/>
                        <xs:enumeration value="no-long-action-in-progress"/>
                        <xs:enumeration value="other-reason"/>
                        <xs:enumeration value="response-code-not-known"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:choice>
        <xs:attribute name="ResponseCode" type="xs:integer" use="required"/>
        <xs:attribute name="Position" type="xs:positiveInteger" use="required"/>
    </xs:complexType>
    <xs:complexType name="StatusASN1">
        <xs:sequence>
            <xs:element name="ASN1Status">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- Generic -->
                        <xs:enumeration value="success"/>
                        <xs:enumeration value="notKnown"/>
                        <!-- Note that error codes in this list may be used in more than one command, however in those cases
														 they will appear in only one of the sub-lists below -->
                        <!-- Provide/Update Security Credentials 6.15.1, 6.21, 6.23, 8.5 -->
                        <xs:enumeration value="badCertificate"/>
                        <xs:enumeration value="noTrustAnchor"/>
                        <xs:enumeration value="insufficientMemory"/>
                        <xs:enumeration value="resourcesBusy"/>
                        <xs:enumeration value="other"/>
                        <!-- Update Device Certificate 6.15.2 -->
                        <xs:enumeration value="invalidCertificate"/>
                        <xs:enumeration value="wrongDeviceIdentity"/>
                        <xs:enumeration value="invalidKeyUsage"/>
                        <xs:enumeration value="noCorrespondingKeyPairGenerated"/>
                        <xs:enumeration value="wrongPublicKey"/>
                        <xs:enumeration value="certificateStorageFailed"/>
                        <xs:enumeration value="privateKeyChangeFailed"/>
                        <!-- Issue Security Credentials 6.17 -->
                        <!-- invalidKeyUsage may also be returned -->
                        <xs:enumeration value="keyPairGenerationFailed"/>
                        <xs:enumeration value="cRProductionFailed"/>
                        <!-- Provide Device Certificate 6.24.1 -->
                        <!-- other may also be returned -->
                        <xs:enumeration value="trustAnchorNotFound"/>
                        <!-- Provide Device Certificate 6.24.2 -->
                        <!-- Note that invalidKeyUsage is used here too -->
                        <xs:enumeration value="noCertificateHeld"/>
                        <xs:enumeration value="certificateRetrievalFailure"/>
                        <!-- Join commands 8.7.1 & 8.7.2 -->
                        <xs:enumeration value="invalidMessageCodeForJoinMethodAndRole"/>
                        <xs:enumeration value="invalidJoinMethodAndRole"/>
                        <xs:enumeration value="incompatibleWithExistingEntry"/>
                        <xs:enumeration value="deviceLogFull"/>
                        <xs:enumeration value="writeFailure"/>
                        <xs:enumeration value="keyAgreementNoResources"/>
                        <xs:enumeration value="keyAgreementUnknownIssuer"/>
                        <xs:enumeration value="keyAgreementUnsupportedSuite"/>
                        <xs:enumeration value="keyAgreementBadMessage"/>
                        <xs:enumeration value="keyAgreementBadKeyConfirm"/>
                        <xs:enumeration value="invalidOrMissingCertificate"/>
                        <xs:enumeration value="noPartnerLinkKeyReceived"/>
                        <xs:enumeration value="noCBKEResponse"/>
                        <!-- Unjoin commands 8.8.1, 8.8.2 -->
                        <xs:enumeration value="otherDeviceNotInDeviceLog"/>
                        <xs:enumeration value="otherFailure"/>
                        <!-- Read Device Join Service 8.9 -->
                        <xs:enumeration value="readFailure"/>
                        <!-- Read Device Join Service 8.12 -->
                        <!-- Note that some of the response codes defined for the join services are used for 8.12 too, namely:
															 incompatibleWithExistingEntry, deviceLogFull & writeFailure -->
                        <!-- Activate Image Code 11.3 -->
                        <xs:enumeration value="noImageHeld"/>
                        <xs:enumeration value="hashMismatch"/>
                        <xs:enumeration value="activationFailure"/>
                        <!-- Synchronise Clock 6.11 -->
                        <xs:enumeration value="reliable"/>
                        <xs:enumeration value="invalid"/>
                        <xs:enumeration value="unreliable"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="ResponseCode" type="xs:integer" use="required"/>
    </xs:complexType>
